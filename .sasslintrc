{
  "options": {
    "formatter": "stylish"
  },
  "files": {
    "include": "web/sass/**/*.sass"
  },
  "rules": {
    "no-ids": 0,
    "force-element-nesting": 0,
    "property-sort-order": 0,
    "shorthand-values": 0,
    "placeholder-in-extend": 0,
    "no-color-literals": 0,
    "no-important": 0,
    "no-color-keywords": 0,
    "empty-line-between-blocks": 0,
    "class-name-format": 0,
    "nesting-depth": 0,
    "force-attribute-nesting": 0,
    "no-qualifying-elements": 0,
    "no-vendor-prefixes": 0,
    "force-pseudo-nesting": 0,
    "zero-unit": 2,
    "single-line-per-selector": 2,
    "url-quotes": 2,
    "quotes": [2, {
      "style": "double"
    }],
    "space-after-comma": 2,
    "pseudo-element": 2,
    "indentation": 2,
    "no-empty-rulesets": 2,
    "hex-notation": 0,
    "hex-length": [2, {
      "style": "long"
    }],
    "no-duplicate-properties": 2,
    "no-transition-all": 0,
    "no-invalid-hex": 0,
    "leading-zero": 0
  }
}
